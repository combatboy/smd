<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:plugin_uexcalendar_attrs_state_range_middle="true">
        <color android:color="@color/plugin_uexcalendar_color_calendar_selected_range_bg" />
    </item>
    <item android:state_selected="true">
        <color android:color="@color/plugin_uexcalendar_color_calendar_selected_day_bg" />
    </item>
    <item android:state_pressed="true">
        <color android:color="@color/plugin_uexcalendar_color_calendar_selected_day_bg" />
    </item>
    <item app:plugin_uexcalendar_attrs_state_current_month="false">
        <color android:color="@color/plugin_uexcalendar_color_calendar_active_month_bg" />
    </item>
    <item app:plugin_uexcalendar_attrs_state_today="true">
        <color android:color="@color/plugin_uexcalendar_color_calendar_active_month_bg" />
    </item>
    <item>
        <color android:color="@color/plugin_uexcalendar_color_calendar_active_month_bg" />
    </item>
</selector>